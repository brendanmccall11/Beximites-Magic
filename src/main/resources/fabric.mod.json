{
	"schemaVersion": 1,
	"id": "beximitesmagic",
	"version": "${version}",
	"name": "Beximite's Magic",
	"description": "This is a magic mod for Minecraft made by Beximite.",
	"authors": [
		"Brendan McCall"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "All rights reserved.",
	"icon": "assets/beximitesmagic/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "net.brendanmccall.beximitesmagic.BeximitesMagic",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "net.brendanmccall.beximitesmagic.BeximitesMagicDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"beximitesmagic.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "1.21.x",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.9.23"
	},
	"suggests": {
		"another-mod": "*"
	}
}